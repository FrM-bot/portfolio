{
	"useTabs": false,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 100,
	"plugins": [
		"prettier-plugin-tailwindcss",
		"prettier-plugin-astro"
	],
	"pluginSearchDirs": [
		"."
	],
	"overrides": [
		{
			"files": "*.astro",
			"options": {
				"parser": "astro"
			}
		}
	],
	"semi": false,
	"tabWidth": 2,
	"tailwindConfig": "./tailwind.config.js"
}